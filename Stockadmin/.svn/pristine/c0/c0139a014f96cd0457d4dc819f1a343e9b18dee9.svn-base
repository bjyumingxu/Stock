public class PoBean {
	
	private  static PoBean  pb =new PoBean(); 
	
	
	private PoBean(){}

	private String name="pobean";
	
	private  Date date ;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
	public   static PoBean getnewInstance(){
		return pb;
	
	}

	public Date getDate() {
		return date;
	}

	public void setDate(Date date) {
		this.date = date;
	}
	
	
	
	
}
